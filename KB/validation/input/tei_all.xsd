        <xs:simpleType>
          <xs:restriction>
            <xs:simpleType>
              <xs:list itemType="ns1:data.pointer"/>
            </xs:simpleType>
            <xs:minLength value="1"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
