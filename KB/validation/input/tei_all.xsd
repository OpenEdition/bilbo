  <xs:element name="specGrp">
    <xs:annotation>
      <xs:documentation>(specification group) contains any convenient grouping of specifications for use within
  the current module.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:group ref="ns1:model.oddDecl"/>
        <xs:group ref="ns1:model.oddRef"/>
        <xs:group ref="ns1:model.divPart"/>
